<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.administrator.androidmanager">

    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.CAMERA"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/android_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
        </activity>
        <activity android:name=".telephone.TelActivity">
        </activity>
        <activity android:name=".ViewPagerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".telephone.TellistActivity">
        </activity>
        <activity android:name=".HomeActivity">
        </activity>
        <activity android:name=".softwaremanager.PackageActivity">
        </activity>
        <activity android:name=".mobiletest.MobileTestActivity">
        </activity>
        <receiver android:name=".mobiletest.MobileTestActivity$BatteryBroadCast"/>
    </application>

</manifest>